extends layout

block prepend title
  | #{ page.title + ' - '}

block metatags
  meta(property="og:title", content=page.title)

block content

  div.ui.segment.dfa-about
    h1.ui.header.aligned.center= page.title
    div.ui.divider
    article.page
      section.content!= typogr(page.html).typogrify()
