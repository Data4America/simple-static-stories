extends layout

block prepend title
  | #{ page.title + ' - '}


block content
  div.container.content-block
    article.page
      include teamGrid
      h1 Our Team
      - var people = env.helpers.getPeople(contents).filter(function (p) { return !p.advisor; });
      mixin teamGrid(people)
      h2 Advisors
      - var people = env.helpers.getPeople(contents).filter(function (p) { return p.advisor; });
      mixin teamGrid(people)
