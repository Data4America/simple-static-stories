{
  "locals": {
    "url": "http://data4america.org",
    "name": "Data4America",
    "development": true
  },
  "plugins": [
    "wintersmith-nap",
    "./plugins/paginator.coffee",
    "./plugins/people.js"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "baseUrl": "/",
  "nap": {
    "assets": {
      "css": {
        "main": ["/css/styles.css"],
        "lifemap": ["/css/lifemap.css"]
      },
      "js": {
        "main": ["/js/jquery-2.1.4.min.js", "/js/d4a.js", "/js/semantic.js"],
        "lifemap": ["/js/jquery-2.1.4.min.js", "/lifemap/js/lodash.js", "/lifemap/js/scrollReveal.js", "/lifemap/js/app.js"]
      }
    }
  }
}
