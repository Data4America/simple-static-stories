{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Data4America"
  },
  "plugins": [
    "./node_modules/wintersmith-nap/src/wintersmith-nap.coffee",
    "./plugins/paginator.coffee",
    "./plugins/people.js"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "baseUrl": "/",
  "nap": {
    "assets": {
      "css": {
        "main": ["/css/*"]
      },
      "js": {
        "main": ["/js/*"]
      }
    }
  }
}
