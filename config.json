{
  "locals": {
    "url": "http://stories.data4america.org",
    "name": "Data4America"
  },
  "plugins": [
    "wintersmith-nap",
    "./plugins/paginator.coffee",
    "./plugins/people.js"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "baseUrl": "/",
  "nap": {
    "assets": {
      "css": {
        "main": ["/css/styles.css"]
      },
      "js": {
        "main": ["/js/jquery-2.1.4.min.js", "/js/d4a.js", "/js/bootstrap.js", "/js/semantic.min.js"]
      }
    }
  }
}
